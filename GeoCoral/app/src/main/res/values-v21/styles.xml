<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="GeoCoral" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/ZcolorPrimary</item>
        <item name="colorPrimaryDark">@color/ZcolorPrimaryDark</item>
        <item name="colorAccent">@color/ZcolorAccent</item>
        <item name="android:colorBackground">@color/ZcolorBG</item>
        <item name="android:statusBarColor">@color/ZdarkYellow</item>
        <item name="android:navigationBarColor">@color/ZcolorYellow</item>
        <item name="android:textColorPrimaryInverse">@color/ZcolorYellow</item>

        <item name="android:button">@color/ZcolorAccent</item>

        <!-- Active thumb color & Active track color(30% transparency) -->
        <item name="colorControlActivated">@color/ZcolorAccent</item>
        <!-- Inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/ZcolorAccent</item>
        <!-- Inactive track color(30% transparency) -->
        <item name="android:colorForeground">@color/ZcolorAccent</item>




    </style>
</resources>